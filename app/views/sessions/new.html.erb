<h1>Log in</h1>

<%= form_with url: sessions_path, local: true do |form| %>


<div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, placeholder: 'Email' %>
</div>

<div class="field">
    <%= form.label :password%>
    <%= form.password_field :password, placeholder: 'Password' %>
</div>

<div class="actions">
    <%= form.submit %>
</div>
<% end %>